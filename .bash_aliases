# programs
alias chrome='google-chrome'
alias dart='~/dart/dart-sdk/bin/dart'
alias darted1='env UBUNTU_MENUPROXY= ~/dart/DartEditor -data ~/dart/workspaces/dart1'
alias darted2='env UBUNTU_MENUPROXY= ~/dart/DartEditor -data ~/dart/workspaces/dart2'
alias darted3='env UBUNTU_MENUPROXY= ~/dart/DartEditor -data ~/dart/workspaces/dart3'
alias e='emacs'
alias eclipse='eclipse44'

# shortcuts
alias ba='emacs ~/.bash_aliases'
alias sb='source ~/.bashrc'
alias xm='emacs ~/.xmonad/xmonad.hs'
alias xb='emacs ~/.xmobarrc'
